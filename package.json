{
  "name": "@orioro/expression",
  "version": "0.0.0-semantic-release",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "scripts": {
    "test": "jest --collectCoverage",
    "dev": "jest --watch",
    "dev:readme": "readme --watch",
    "build": "rm -rf dist && rollup --config ./rollup/build.config.js && npm run tsc-declaration",
    "prepare": "npm run build && readme",
    "update-dependencies": "npx npm-check-updates -u",
    "tsc-declaration": "tsc --declaration --emitDeclarationOnly --declarationDir dist",
    "commit": "cz"
  },
  "author": "",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@orioro/readme": "^0.3.2",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@types/jest": "^26.0.14",
    "babel-jest": "^26.3.0",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "jest": "^26.4.2",
    "rollup": "^2.26.8",
    "semantic-release": "^17.3.7",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@orioro/validate-type": "^1.1.0",
    "lodash": "^4.17.20",
    "luxon": "^1.25.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  }
}
